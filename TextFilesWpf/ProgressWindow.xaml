<Window x:Class="TextFilesWpf.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TextFilesWpf"
        mc:Ignorable="d"
        Title="Прогресс" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Width="300" Height="200">
        <TextBlock Name="passed" HorizontalAlignment="Left" Margin="33,0,0,40" VerticalAlignment="Center" Text="TextBlock" TextWrapping="Wrap"/>
        <TextBlock Name="total" HorizontalAlignment="Left" Margin="33,40,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Center"/>

    </Grid>
</Window>
